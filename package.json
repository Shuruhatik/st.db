{
  "name": "st.db",
  "version": "7.2.6",
  "description": "ST.db is a versatile npm package for managing databases with a simple key-value interface. It supports various database types, offers seamless switching between drivers, efficient caching, encryption, and customizable configurations. Ideal for diverse projects with clear documentation.",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "require": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./src/drivers/*": "./src/drivers/*.js",
    "./src/drivers/*.js": {
      "types": "./src/drivers/*.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuruhatik/st.db"
  },
  "bugs": {
    "url": "https://github.com/Shuruhatik/st.db/issues"
  },
  "homepage": "https://db.shuruhatik.com/",
  "files": [
    "/src"
  ],
  "keywords": [
    "st.db",
    "json",
    "yaml",
    "yml",
    "key-value",
    "db",
    "encryption",
    "encrypt",
    "st",
    "discord",
    "database",
    "documentation",
    "backend",
    "mongo",
    "mongodb",
    "firebase",
    "mysql",
    "sqlite3",
    "xml",
    "firestore",
    "postgres",
    "json",
    "map",
    "shuruhatik"
  ],
  "scripts": {
    "start": "node index.js"
  },
  "author": "shuruhatik",
  "license": "CC BY-NC-ND 4.0",
  "devDependencies": {
    "@types/node": "^18.7.6",
    "yaml": "2.x"
  }
}
